{
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": [
            "jsdoc",
            "closure"
        ]
    },
    "source": {
        "includePattern": ".+\\.js(doc|x)?$",
        "include": [
            "."
        ],
        "exclude": [
            "node_modules",
            "build",
            "docs",
            "out"
        ]
    },
    "recurseDepth": 10,
    "opts": {
        "destination": "./docs/",
        "recurse": true
    }
}